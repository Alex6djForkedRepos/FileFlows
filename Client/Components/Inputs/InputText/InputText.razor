@namespace FileFlows.Client.Components.Inputs
@inherits Input<string>

<InputWrapper Input="@this">
    <div>
        <input type="text" id=@Uid @bind-value="Value" @bind-value:event="oninput" autocomplete="off" 
        readonly="@(ReadOnly ? "readonly" : null)" placeholder="@Placeholder"  />
    </div>
</InputWrapper>
