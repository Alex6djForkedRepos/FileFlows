@namespace FileFlows.Client.Components.Dashboard


<div class="portlet line-data">
    <div class="title">
        @if (string.IsNullOrEmpty(Icon) == false)
        {
            <i class="@Icon"></i>
        }
        <span>@Title</span>
    </div>
    <div class="content">
        
        <ApexChart TItem="BoxPlotData">
            <ApexBoxPlotSeries TItem="BoxPlotData"
                        Items="Data"
                        XValue="@(e => e.Name)"
                        YValue="@(e => /*new [] {e.Minimum, e.LowQuartile, e.Median, e.HighQuartile, e.Maximum}*/ e.Median)" />
        </ApexChart>
    </div>
</div>