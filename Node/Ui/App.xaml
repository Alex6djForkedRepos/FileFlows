<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 
             x:Class="FileFlows.Node.Ui.App">
    <Application.Styles>
        <StyleInclude Source="avares://Avalonia.Themes.Default/DefaultTheme.xaml"/>
        <StyleInclude Source="avares://Avalonia.Themes.Default/Accents/BaseDark.xaml"/>
    </Application.Styles>
    <Application.Resources>
        <SolidColorBrush x:Key="ToggleSwitchFillOn" Color="#ff0090" />
        <SolidColorBrush x:Key="ToggleSwitchFillOnPointerOver" Color="#ff0090" />
        <SolidColorBrush x:Key="ToggleSwitchFillOnPressed" Color="#ff0090" />
        <SolidColorBrush x:Key="ToggleSwitchStrokeOn" Color="#ff0090" />
        <SolidColorBrush x:Key="ToggleSwitchStrokeOnPointerOver" Color="#ff0090" />
        <SolidColorBrush x:Key="ToggleSwitchStrokeOnPressed" Color="#ff0090" />
        <SolidColorBrush x:Key="ToggleSwitchStrokeOnDisabled" Color="#ff0090" />
    </Application.Resources>
</Application>